{% if settings.facebook_url != blank %}
  <li>
    <a href="{{ settings.facebook_url }}" target="_blank" title="Facebook">
      {% comment %} {% include 'svg-icon-facebook' %} {% endcomment %}
      {% include 'svg-icon-facebook2' %}
    </a>
  </li>
{% endif %}

{% if settings.instagram_id != blank %}
    <li>
      <a href="//instagram.com/{{ settings.instagram_id }}" target="_blank" title="Instagram">
        {% include 'svg-icon-instagram' %}
      </a>
    </li>
{% endif %}

